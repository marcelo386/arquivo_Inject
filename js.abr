function replacer(key, value) {
    if (key === "msgs") {
        return "none";
    } else {
        return value;
    }
}

function arrayToJson(array) {
    var result = {};
    for (var i = 0; i < array.length; i++) {
        var currentObj = array[i];
        for (var key in currentObj) {
            if (currentObj.hasOwnProperty(key)) {
                result[key] = currentObj[key];
            }
        }
    }
    return result;
}

