window.WPP.SendTextMessageNew = async function (wid, texto, options, xSeuID = '', xSeuID2 = '', xSeuID3 = '', xSeuID4 = '') {
  var isGroup = wid.includes('@g.us');
  try {
    const validar = isGroup ? { wid: { _serialized: wid } } : await WPP.contact.queryExists(wid);

    if (validar !== null && validar !== undefined) {
      let result;   
      SetConsoleMessage("CheckNumberExists",    JSON.stringify({ NumberOriginal : wid, id : validar.wid._serialized, valid : true}));

      result = await WPP.chat.sendTextMessage(validar.wid._serialized, texto, options);

      if (result !== undefined && result !== null) {
        //return result;

        try {
          Obj = {
            Seuid: xSeuID,
            Seuid2: xSeuID2,
            Seuid3: xSeuID3,
            Seuid4: xSeuID4,
            JsonMessage: JSON.stringify(result),
          };

          SetConsoleMessage("sendTextMessageEx", JSON.stringify(Obj));
        } catch (err) {
          console.log(err);
        }
        return result;
      } else {
        //console.error("A mensagem nÃƒÂ£o pÃƒÂ´de ser enviada.");        
        var RetError = "A mensagem nÃƒÂ£o pÃƒÂ´de ser enviada.";
        Obj = {
          chatid: wid,
          Seuid: xSeuID,
          Seuid2: xSeuID2,
          Seuid3: xSeuID3,
          Seuid4: xSeuID4,
          RetEvent: "sendTextMessageNew",
          statusCode: 300,
          Error: RetError,
        };
        SetConsoleMessage("ErrorResponse", JSON.stringify(Obj));
        //return error;

        return null;
      }
    } else {
      //console.error("O contato nÃƒÂ£o existe ou ÃƒÂ© invÃƒÂ¡lido.");
      var RetError = "O contato nÃƒÂ£o existe ou ÃƒÂ© invÃƒÂ¡lido.";
      Obj = {
        chatid: wid,
        Seuid: xSeuID,
        Seuid2: xSeuID2,
        Seuid3: xSeuID3,
        Seuid4: xSeuID4,
        RetEvent: "sendTextMessageNew",
        statusCode: 301,
        Error: RetError,
      };
      SetConsoleMessage("ErrorResponse", JSON.stringify(Obj));
      SetConsoleMessage("CheckNumberExists", JSON.stringify({ NumberOriginal : wid, id : wid, valid : false}));

      return null;
    }
  } catch (error) {
    // Verificar se o erro ÃƒÂ© causado pelo contato bloqueado
    if (error.name === "ContactBlocked") {
      //console.log("Contato estÃƒÂ¡ bloqueado:", error.contact);
      // FaÃƒÂ§a alguma aÃƒÂ§ÃƒÂ£o apropriada, por exemplo, exiba uma mensagem de erro ou registre o erro
      var RetError = "O contato nÃƒÂ£o existe ou ÃƒÂ© invÃƒÂ¡lido.";
      Obj = {
        chatid: wid,
        Seuid: xSeuID,
        Seuid2: xSeuID2,
        Seuid3: xSeuID3,
        Seuid4: xSeuID4,
        RetEvent: "sendTextMessageNew",
        statusCode: 302,
        Error: RetError,
      };
      SetConsoleMessage("ErrorResponse", JSON.stringify(Obj));      
    } else {
      // Lidar com outros erros que podem ocorrer
      //console.error("Erro desconhecido:", error);
      var RetError = error.message ? error.message : (error.stack || "Unknown Error");
      Obj = {
        chatid: wid,
        Seuid: xSeuID,
        Seuid2: xSeuID2,
        Seuid3: xSeuID3,
        Seuid4: xSeuID4,
        RetEvent: "sendTextMessageNew",
        statusCode: 303,
        Error: RetError,
      };
      SetConsoleMessage("ErrorResponse", JSON.stringify(Obj));      
    }
    // Retorne um valor padrÃƒÂ£o ou null para evitar que a aplicaÃƒÂ§ÃƒÂ£o quebre
    return null;
  }
};

