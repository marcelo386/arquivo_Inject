function isChatMessage(message) {
    if (message.isSentByMe) {
        return false;
    }
    if (message.isNotification) {
        return false;
    }
    if (!message.isUserCreatedType) {
        return false;
    }
    return true;
};

window.WAPI.getUnreadMessages = async function(includeMe, includeNotifications, use_unread_count, done) {
//window.WAPI.getUnreadMessages = function(includeMe, includeNotifications, use_unread_count, done) {
    //const chats = WPP.whatsapp.Chat.models;
    
    const options = {
        onlyWithUnreadMessage: true,    
        onlyUsers: false 
      };
        
    var chats = await WPP.chat.list(options);

    if (chats) {
        try {
          for (var i = 0; i < chats.length; i++) {
              chats[i] = chats[i].toJSON();  

              let isGroup = chats[i].id._serialized.includes('@g.us');
              chats[i].isGroup = isGroup;
              chats[i].formattedTitle = chats[i].name;

              /*A consulta buscando o contato foi desativado para melhorar a performance*/
              var contacts = await WPP.contact.get(chats[i].id._serialized);
	          var profilePhoto = await WPP.contact.getProfilePictureUrl(chats[i].id._serialized);

              // Verifique se os contatos existem e tÃƒÂªm comprimento maior que zero
              if (contacts && Object.keys(contacts).length > 0) {   
                  chats[i].contact = {
                      id: contacts.id,
                      name: contacts.name,
                      pushname: contacts.name,
                      formattedName: contacts.name,
                      profilePicThumb: profilePhoto,
                      isBusiness: contacts.isBusiness,
                      isEnterprise: contacts.isEnterprise
                    };
                  //console.log(`Chats[${i}] com propriedade "contact":`, chats[i]);
              } else{
                  chats[i].contact = {
	              id: chats[i].id._serialized,
	              name: chats[i].name,
	              pushname: chats[i].name,
	              formattedName: chats[i].name,
	              profilePicThumb: profilePhoto 
	            }
            }
              
              //let profilePhoto = ".";

              //Criado Contact passando dados Basicos para manter a compatibilidade, como a versÃƒÂ£o anterior
              /*chats[i].contact = {
                id: chats[i].id._serialized,
                name: chats[i].name,
                pushname: chats[i].name,
                formattedName: chats[i].name,
                profilePicThumb: profilePhoto                
              };*/

              //Deletar JSON Array das Mensagens do nÃƒÂ³ msgs, buscar logo abaixo as Mensagens NÃƒÂ£o Lidas deste Chat
              delete chats[i].msgs;
          }
          
        } catch (e) {
          //console.log('Erro:'.concat(e));
        }
    } else {
      //console.log('VAZIO');
    }
         
    //const chats = WPP.whatsapp.Chat.getModelsArray(); deprecated
    var output = [];

    for (var chat in chats) {
        if (isNaN(chat)) {
            continue;
        }

        var messageGroupObj = chats[chat];
        var messageGroup = chats[chat]; //WAPI._serializeChatObj(messageGroupObj);

        messageGroup.messages = [];

        //const messages = messageGroupObj.msgs._models; deprecated       
        var messages = await WPP.chat.getMessages(chats[chat].id._serialized, {count: -1, onlyUnread: true});
        
        for (let i = messages.length - 1; i >= 0; i--) {
            
            var messageObj = messages[i];

            messageObj.isNewMsg = false;
                
            var message = WAPI.processMessageObj(messageObj, includeMe, false); //includeNotifications);// MUDAR PARA "FALSE" AQUI
            if (message) {
                messageGroup.messages.push(message);
            }

            /*
            if (typeof(messageObj.isNewMsg) != "boolean" || messageObj.isNewMsg === false) {
                continue;
            } else {
                messageObj.isNewMsg = false;
                
                let message = WAPI.processMessageObj(messageObj, includeMe, false); //includeNotifications);// MUDAR PARA "FALSE" AQUI
                if (message) {
                    messageGroup.messages.push(message);
                }
            }*/
        }

        if (messageGroup.messages.length > 0) {
            output.push(messageGroup);
        } else { // no messages with isNewMsg true
            if (use_unread_count) {
                let n = messageGroupObj.unreadCount; // usara o atributo unreadCount para buscar as ultimas n mensagens do remetente
                for (let i = messages.length - 1; i >= 0; i--) {
                    let messageObj = messages[i];
                    if (n > 0) {
                        if (!messageObj.fromMe) {
                            let message = WAPI.processMessageObj(messageObj, includeMe, includeNotifications);
                            messageGroup.messages.unshift(message);
                            n -= 1;
                        }
                    } else if (n === -1) { // chat was marked as unread so will fetch last message as unread
                        if (!messageObj.fromMe) {
                            let message = WAPI.processMessageObj(messageObj, includeMe, includeNotifications);
                            messageGroup.messages.unshift(message);
                            break;
                        }
                    } else { // unreadCount = 0
                        break;
                    }
                }
                if (messageGroup.messages.length > 0) {
                    messageGroupObj.unreadCount = 0; // reset unread counter
                    output.push(messageGroup);
                }
            }
        }
    }
    if (done !== undefined) {
        done(output);

    }
	
	//mike teste 16/02/2021 tentativa de retornar imagem de perfil
    SetConsoleMessage("getUnreadMessages", JSON.stringify(output));
    return output;
    
};

