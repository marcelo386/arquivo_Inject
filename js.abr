async function deleteOldChats(QtdChatsExcluir) {
    const options = {
        onlyWithUnreadMessage: false,
        onlyUsers: true
    };

    try {
        const chats = await WPP.chat.list(options);

        // Ordene os chats de forma inversa (do mais recente para o mais antigo)
        chats.reverse();

        
        const numChatsToDelete = Math.min(QtdChatsExcluir, chats.length);

        for (let i = 0; i < numChatsToDelete; i++) {
            await WPP.chat.delete(chats[i].id._serialized).then(result => {
                //console.log('Chat ${chats[i].id._serialized} excluÃ­do:', result);
            });
        }
    } catch (e) {
        console.error('Erro:', e);
    }
};

